{
    "version": 2,
    "builds": [
       {
         "src": "client_2/package.json",
         "use": "@vercel/static-build",
         "config": {
           "distDir": "build"
         }
       },
       {
         "src": "server/index.js",
         "use": "@vercel/node"
       }
    ],
    "routes": [
       {
         "src": "/api/(.*)",
         "dest": "server/index.js"
       },
       {
         "src": "/(.*)",
         "dest": "client_2/$1"
       }
    ]
   }
   